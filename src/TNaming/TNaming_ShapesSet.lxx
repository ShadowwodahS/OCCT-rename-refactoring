// purpose  :
//=======================================================================

inline Standard_Integer ShapesSet::NbShapes1() const
{
  return myMap.Extent();
}

//=======================================================================
// function : ChangeMap
// purpose  :
//=======================================================================

inline TopTools_MapOfShape& ShapesSet::ChangeMap()
{
  return myMap;
}


// function : Standard_Boolean
// purpose  :
//=======================================================================

inline Standard_Boolean ShapesSet::Contains(const TopoShape& S) const
{
  return myMap.Contains(S);
}

//=======================================================================
// function : Standard_Boolean
// purpose  :
//=======================================================================

inline Standard_Boolean ShapesSet::Remove(const TopoShape& S)
{
  return myMap.Remove(S);
}

//=======================================================================
// function : IsEmpty
// purpose  :
//=======================================================================

inline Standard_Boolean ShapesSet::IsEmpty() const
{
  return myMap.IsEmpty();
}

//=======================================================================

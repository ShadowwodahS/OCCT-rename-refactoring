inline void ElSLib1::D3(const Standard_Real U,
                       const Standard_Real V,
                       const Cone1&      C,
                       Point3d&             P,
                       Vector3d&             Vu,
                       Vector3d&             Vv,
                       Vector3d&             Vuu,
                       Vector3d&             Vvv,
                       Vector3d&             Vuv,
                       Vector3d&             Vuuu,
                       Vector3d&             Vvvv,
                       Vector3d&             Vuuv,
                       Vector3d&             Vuvv)
{

  ElSLib1::ConeD3(U,
                 V,
                 C.Position1(),
                 C.RefRadius(),
                 C.SemiAngle(),
                 P,
                 Vu,
                 Vv,
                 Vuu,
                 Vvv,
                 Vuv,
                 Vuuu,
                 Vvvv,
                 Vuuv,
                 Vuvv);
}


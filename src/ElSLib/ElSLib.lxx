#include <gp_Pln.hxx>
#include <gp_Sphere.hxx>
#include <gp_Torus.hxx>
#include <gp_Cylinder.hxx>
#include <gp_Cone.hxx>
#include <gp_Ax3.hxx>

inline Point3d ElSLib1::Value(const Standard_Real U, const Standard_Real V, const gp_Pln& Pl)
{

  return ElSLib1::PlaneValue(U, V, Pl.Position1());
}

inline Point3d ElSLib1::Value(const Standard_Real U, const Standard_Real V, const Cone1& C)
{

  return ElSLib1::ConeValue(U, V, C.Position1(), C.RefRadius(), C.SemiAngle());
}

inline Point3d ElSLib1::Value(const Standard_Real U, const Standard_Real V, const Cylinder1& C)
{

  return ElSLib1::CylinderValue(U, V, C.Position1(), C.Radius());
}

inline Point3d ElSLib1::Value(const Standard_Real U, const Standard_Real V, const Sphere3& S)
{

  return ElSLib1::SphereValue(U, V, S.Position1(), S.Radius());
}

inline Point3d ElSLib1::Value(const Standard_Real U, const Standard_Real V, const gp_Torus& T)
{

  return ElSLib1::TorusValue(U, V, T.Position1(), T.MajorRadius(), T.MinorRadius());
}

